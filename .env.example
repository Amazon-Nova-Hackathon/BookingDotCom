# ============================================
# Booking Voice Agent - Environment Variables
# ============================================

# --- AWS Credentials (Nova Sonic 2 S2S) ---
# Used for the voice pipeline (Speech-to-Speech)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=ap-northeast-2
# Optional: AWS profile name (alternative to explicit keys)
# AWS_PROFILE=sandbox_ttf
# Optional: Session token for temporary credentials
# AWS_SESSION_TOKEN=

# --- AWS Credentials for Browser-Use (Nova Lite 2 via Bedrock) ---
# Used for the browser automation agent
BEDROCK_ACCESS_KEY_ID=your_bedrock_access_key_here
BEDROCK_SECRET_ACCESS_KEY=your_bedrock_secret_key_here
BEDROCK_REGION=ap-southeast-1
BEDROCK_MODEL_ID=amazon.nova-lite-v2:0
# Optional: AWS profile for Bedrock
# BEDROCK_PROFILE=sandbox_ttf

# --- Service URLs ---
BROWSER_SERVICE_URL=http://localhost:7863
HOST=0.0.0.0
PORT=7860

# --- Browser Agent Configuration ---
BOOKING_URL=https://www.booking.com

# --- WebRTC ICE Servers ---
STUN_SERVER=stun:stun.l.google.com:19302
TURN_SERVER=turn:openrelay.metered.ca:80
TURN_USERNAME=openrelayproject
TURN_CREDENTIAL=openrelayproject

# --- Optional: LangSmith Tracing ---
# LANGCHAIN_TRACING_V2=true
# LANGCHAIN_API_KEY=your_langsmith_key
# LANGCHAIN_PROJECT=booking-voice-agent
